#include <iostream>

using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);

    int T, k;

    cin >> T;

    for (int i = 0; i < T; i++) {
        cin >> k;

        for (int j = 0; j < k; j++) {
            cout << "=";
        }

        cout << '\n';
    }

    return 0;
}
