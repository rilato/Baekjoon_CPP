// 15649와 비슷해 보이지만, bool 배열을 사용하지 않는다.
// 백트래킹의 구조를 파악한다.

#include<iostream>
#include<vector>

using namespace std;

int N, M;
vector <int> v;

void backTracking();

int main(void) {
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);

    cin >> N >> M;
    for (int i = 1; i <= N; i++) {
        v.push_back(i);
        backTracking();
        v.pop_back();
    }
}

void backTracking() {
    if (v.size() == M) {
        for (auto &k : v)
            cout << k << " ";
        cout << "\n";
        return;
    }

    for (int i = v.back() + 1; i <= N; i++) {
        v.push_back(i);
        backTracking();
        v.pop_back();
    }
}
