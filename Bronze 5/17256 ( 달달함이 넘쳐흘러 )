#include <iostream>

using namespace std;

int main(void) {
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    int ax, ay, az, bx, by, bz, cx, cy, cz;

    cin >> ax >> ay >> az >> cx >> cy >> cz;

    bx = cx - az;
    by = cy / ay;
    bz = cz - ax;

    cout << bx << ' ' << by << ' ' << bz;
}
